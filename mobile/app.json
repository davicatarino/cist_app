{
  "expo": {
    "name": "CIST",
    "slug": "cist-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "jsEngine": "hermes",
    "sdkVersion": "50.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
        "**/*"
      ],
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#68ccd1"
      },
    "android": {
      "permissions": ["android.permission.DETECT_SCREEN_CAPTURE"]
    }
  }
}
